        <record id="ir_cron_module_mes_sync_users" model="ir.cron">
            <field name="name">TS002 Sync Users From Mes</field>
            <field eval="False" name="active" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(minutes=5)).strftime('%Y-%m-%d %H:%M:%S')" />
            <field eval="False" name="doall" />
            <field eval="'mes.api'" name="model" />
            <field eval="'sync_users'" name="function" />
            <field eval="'()'" name="args" />
            <field name="priority">5</field>
        </record>

